  "scripts": {
    "build": "tsc",
    "start": "node dist/src/server.js",
    "dev": "ts-node src/server.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "db:migrate": "sequelize-cli db:migrate",
    "db:migrate:undo": "sequelize-cli db:migrate:undo",
    "db:migrate:undo:all": "sequelize-cli db:migrate:undo:all",
    "db:seed": "sequelize-cli db:seed:all",
    "db:seed:undo": "sequelize-cli db:seed:undo:all",
    "migration:create": "sequelize-cli migration:generate --name",
    "migration:run": "sequelize-cli db:migrate",
    "seed:create": "sequelize-cli seed:generate --name",
    "lint": "eslint . --ext .ts",
    "lint-fix": "eslint . --ext .ts --fix",
    "format": "prettier --write .",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:stop": "docker-compose stop",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:migrate": "docker-compose exec app npx sequelize-cli db:migrate",
    "docker:migrate:undo:all": "docker-compose exec app npx sequelize-cli db:migrate:undo:all",
    "docker:seed": "docker-compose exec app npx sequelize-cli db:seed:all",
    "docker:seed:all": "docker-compose exec app npx sequelize-cli db:seed:undo:all"
  },
